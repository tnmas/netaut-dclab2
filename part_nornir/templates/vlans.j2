vlan 4
  name Vlan4
vlan 5
  name Vlan5
!

{% for i in interfaces %}
  {% for int in int_access %}
    {% if int.name ==  i and int.id == "1" and int.hostname == hostname%}
      interface {{i}}
        switchport mode access
        switchport access vlan 4
    {%elif int.name ==  i and int.id == "2" and int.hostname == hostname%}
      interface {{i}}
        switchport mode access
        switchport access vlan 5
    {% else %}
      interface {{i}}
        switchport mode trunk
        switchport trunk encapsulation dot1q
    {% endif %}
  {% endfor %}
{% endfor %}
!
